{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aminadha/connector-php"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "~5.3.0",
        "doctrine/dbal": "^2.5",
        "laravelcollective/html": "^5.3",
        "anahkiasen/former": "^4.0",
        "cviebrock/eloquent-sluggable": "^4.0",
        "venturecraft/revisionable": "^1.28",
        "irazasyed/laravel-identicon": "~1.1",
        "yajra/laravel-datatables-oracle": "^6.22",
        "baum/baum": "~1.1",
        "sands/asasi-http": "~0.1",
        "sands/asasi-policy": "~0.2",
        "sands/asasi-service": "~0.2",
        "thomaswelton/laravel-gravatar": "~1.0",
        "gloudemans/shoppingcart": "^2.1",
        "sands/uploadable": "^0.1.1",
        "barryvdh/laravel-snappy": "^0.3.1",
        "predis/predis": "^1.1",
        "dhtmlx/connector-php": "dev-custom",
        "maatwebsite/excel": "^2.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "sands/asasi-scaffold": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "App\\Mailers\\": "app/Mailers/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
